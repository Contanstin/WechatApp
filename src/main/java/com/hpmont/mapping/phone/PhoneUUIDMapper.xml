<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hpmont.dao.mapper.PhoneUUIDMapper" >
  <resultMap id="BaseResultMap" type="com.hpmont.domain.phone.PhoneUUID" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="phone_number" property="phoneNumber" jdbcType="VARCHAR" />
    <result column="uuid" property="uuid" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, phone_number, uuid
  </sql>



  <select id="isExistUUID" parameterType="com.hpmont.domain.phone.PhoneUUID" resultType="java.lang.Boolean">
    <![CDATA[ select count(id) from phone_uuid
      where phone_number = #{phoneNumber}
      AND  uuid =#{uuid}
    ]]>
  </select>

  <select id="getUUIDCount" parameterType="java.lang.String" resultType="java.lang.Integer">
    select count(1) from phone_uuid where phone_number = #{phoneNumber}
  </select>

  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer" >
    delete from phone_uuid
    where id = #{id,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="com.hpmont.domain.phone.PhoneUUID" >
    insert into phone_uuid (id, phone_number, uuid
      )
    values (#{id,jdbcType=INTEGER}, #{phoneNumber,jdbcType=VARCHAR}, #{uuid,jdbcType=VARCHAR}
      )
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="com.hpmont.domain.phone.PhoneUUID" >
    update phone_uuid
    <set >
      <if test="phoneNumber != null" >
        phone_number = #{phoneNumber,jdbcType=VARCHAR},
      </if>
      <if test="uuid != null" >
        uuid = #{uuid,jdbcType=VARCHAR},
      </if>
    </set>
    where id = #{id,jdbcType=INTEGER}
  </update>
</mapper>